{
    "name": "hemant/zipper",
    "type": "library",
    "description": "This is a little neat helper for the ZipArchive methods with handy functions",
    "keywords": ["laravel", "ZIP", "Archive"],
    "homepage": "https://github.com/Hemantkumawat",
    "license": "MIT",
    "authors": [
        {
            "name": "Hemant Kumawat",
            "email": "hemantkumawat275@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-zip": "*",
        "illuminate/support": ">=5.0",
        "illuminate/filesystem": ">=5.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.7",
        "mockery/mockery": ">=0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Hemant\\Zipper\\": "src/Hemant/Zipper"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hemant\\Zipper\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hemant\\Zipper\\ZipperServiceProvider"
            ],
            "aliases": {
                "Zipper": "Hemant\\Zipper\\Zipper"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
